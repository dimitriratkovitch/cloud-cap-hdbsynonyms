{
  "name": "capmt",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^4",
    "@sap/cds-mtx": "^1",
    "@sap/hana-client": "^2.6.58",
    "@sap/hdi-deploy": "^3.11.4",
    "@sap/instance-manager": "^2",
    "@sap/xsenv": "^3.0.0",
    "@sap/xssec": "^2.2.5",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "cfenv": "1.0.0",
    "express": "^4",
    "passport": "^0.4.1",
    "qs": "^6.9.4",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "npx cds run",
    "deploy:cf": "cf deploy mta_archives/capmt_0.0.1.mtar --retries 0",
    "build": "npx cds build/all --clean",
    "dbbuild": "cds build && cd ./db && npm start"
  },
  "cds": {
    "comment": "Anything defined outside of a [profile] will apply to all profiles.",
    "mtx": {
      "app": {
        "urlpart": "-dev-capmt-app.cfapps.eu.hana.ondemand.com"
      },
      "api": {
        "model": true,
        "provisioning": true,
        "metadata": true
      },
      "element-prefix": [
        "Z_",
        "ZZ_"
      ],
      "namespace-blacklist": [
        "com.sap.",
        "sap."
      ],
      "entity-whitelist": [
        "my.bookshop.Books"
      ],
      "service-whitelist": []
    },
    "odata": {
      "version": "v4"
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "features": {
      "snapi": true
    },
    "[development]": {
      "comment": "Anything defined inside of [development] will overrided prior values.",
      "requires": {
        "db": {
          "kind": "hana",
          "model": [
            "db",
            "srv"
          ],
          "multiTenant": true,
          "vcap": {
            "label": "service-manager"
          }
        },
        "uaa": {
          "kind": "xsuaa"
        },
        "auth": {
          "strategy": "JWT"
        }
      }
    },
    "[production]": {
      "comment": "Anything defined inside of [production] will overrided prior values.",
      "hana": {
        "deploy-format": "hdbtable"
      },
      "requires": {
        "db": {
          "kind": "hana"
        },
        "uaa": {
          "kind": "xsuaa"
        }
      }
    }
  }
}
