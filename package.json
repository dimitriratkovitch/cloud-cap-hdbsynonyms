{
  "name": "capmt",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/audit-logging": "^3.2.0",
    "@sap/cds": "^4",
    "@sap/hana-client": "^2.6.58",
    "@sap/xsenv": "^3.1.0",
    "@sap/xssec": "^2.2.5",
    "express": "^4",
    "hdb": "^0.18.2",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@sap/hdi-deploy": "3.11.15",
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "npx cds run",
    "build_deploy:cf": "mbt build && cf deploy mta_archives/core_0.0.1.mtar --retries 0",
    "build": "npx cds build/all --clean"
  },
  "cds": {
    "[development]": {
      "comment": "Anything defined inside of [development] will overrided prior values.",
      "hana": {
        "deploy-format": "hdbtable"
      },
      "requires": {
        "db": {
          "kind": "hana"
        },
        "uaa": {
          "kind": "xsuaa"
        },
        "auth": {
          "strategy": "mock"
        }
      }
    },
    "[production]": {},
    "requires": {
      "db": {
        "kind": "sql"
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  }
}